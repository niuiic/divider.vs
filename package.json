{
  "name": "divide-line",
  "displayName": "divider",
  "description": "custom divide lines",
  "version": "0.1.0",
  "publisher": "niuiic",
  "keywords": [
    "vscode",
    "plugin",
    "divider"
  ],
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/niuiic/divider.vs"
  },
  "homepage": "https://github.com/niuiic/divider.vs",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "divider",
      "properties": {
        "divider.divider": {
          "type": "array",
          "default": [],
          "description": "config for per level divider"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "divider",
          "title": "divider",
          "icon": "icon.png"
        }
      ]
    },
    "views": {
      "divider": [
        {
          "id": "divider",
          "name": "divider"
        }
      ]
    },
    "commands": [
      {
        "command": "cursorToLine",
        "title": "move cursor to line"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "publish": "pnpm compile && pnpm vsce publish --no-dependencies",
    "package": "pnpm compile && pnpm vsce package --no-dependencies"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "~18.14.6",
    "@types/vscode": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vscode/test-electron": "^2.3.0",
    "@vscode/vsce": "^2.18.0",
    "eslint": "^8.35.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  }
}
